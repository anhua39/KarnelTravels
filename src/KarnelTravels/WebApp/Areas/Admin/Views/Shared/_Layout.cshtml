<!DOCTYPE html>
<html lang="en" class="light-style layout-menu-fixed layout-compact" dir="ltr" data-theme="theme-default" data-assets-path="~/admin/" data-template="vertical-menu-template-free">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title>@ViewData["Title"] - Karnel Travels Management</title>
    <partial name="~/Areas/Admin/Views/Shared/Sections/_Styles.cshtml" />
    @await RenderSectionAsync("Styles", required: false)
</head>
<body>
    <div class="layout-wrapper layout-content-navbar">
        <div class="layout-container">
            <partial name="~/Areas/Admin/Views/Shared/Sections/Navbar/_Sidenav.cshtml" />
            <div class="layout-page">
                <partial name="~/Areas/Admin/Views/Shared/Sections/Navbar/_Topnav.cshtml" />
                <div class="container-xxl flex-grow-1 container-p-y">
                    @RenderBody()
                    <button type="button" class="btn btn-danger btn-floating btn-lg" id="btn-back-to-top">
                        <i class="fa-solid fa-arrow-up"></i>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <partial name="~/Areas/Admin/Views/Shared/Sections/_Scripts.cshtml" />
</body>
</html>
